{
    "id": "rsnapshot",
    "name": "rsnapshot",
    "icon": "https://raw.githubusercontent.com/walkxcode/dashboard-icons/main/png/rsnapshot.png",
    "links": [
        {
            "type": "docker",
            "url": "https://hub.docker.com/r/linuxserver/rsnapshot"
        },
        {
            "type": "docs",
            "url": "https://docs.linuxserver.io/images/docker-rsnapshot"
        }
    ],
    "containers": [
        {
            "name": "rsnapshot",
            "image": "linuxserver/rsnapshot",
            "env": [
                {
                    "id": "PUID",
                    "key": "PUID",
                    "description": "User ID",
                    "default": "100"
                },
                {
                    "id": "PGID",
                    "key": "PGID",
                    "description": "Group ID",
                    "default": "100"
                },
                {
                    "id": "TZ",
                    "key": "TZ",
                    "description": "Timezone",
                    "default": "America/New_York"
                }
            ],
            "volumes": [
                {
                    "container": "/config",
                    "description": "Image exposes volume mounted at /config",
                    "key": "config"
                }
            ],
            "ports": []
        }
    ]
}