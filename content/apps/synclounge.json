{
    "id": "synclounge",
    "name": "synclounge",
    "icon": "https://raw.githubusercontent.com/walkxcode/dashboard-icons/main/png/synclounge.png",
    "links": [
        {
            "type": "docker",
            "url": "https://hub.docker.com/r/linuxserver/synclounge"
        },
        {
            "type": "docs",
            "url": "https://docs.linuxserver.io/images/docker-synclounge"
        }
    ],
    "containers": [
        {
            "name": "synclounge",
            "image": "linuxserver/synclounge",
            "env": [
                {
                    "id": "PUID",
                    "key": "PUID",
                    "description": "User ID",
                    "default": "100"
                },
                {
                    "id": "PGID",
                    "key": "PGID",
                    "description": "Group ID",
                    "default": "100"
                },
                {
                    "id": "TZ",
                    "key": "TZ",
                    "description": "Timezone",
                    "default": "America/New_York"
                }
            ],
            "volumes": [],
            "ports": []
        }
    ]
}