{
    "id": "webgrabplus",
    "name": "webgrabplus",
    "icon": "https://raw.githubusercontent.com/walkxcode/dashboard-icons/main/png/webgrabplus.png",
    "links": [
        {
            "type": "docker",
            "url": "https://hub.docker.com/r/linuxserver/webgrabplus"
        },
        {
            "type": "docs",
            "url": "https://docs.linuxserver.io/images/docker-webgrabplus"
        }
    ],
    "containers": [
        {
            "name": "webgrabplus",
            "image": "linuxserver/webgrabplus",
            "env": [
                {
                    "id": "PUID",
                    "key": "PUID",
                    "description": "User ID",
                    "default": "100"
                },
                {
                    "id": "PGID",
                    "key": "PGID",
                    "description": "Group ID",
                    "default": "100"
                },
                {
                    "id": "TZ",
                    "key": "TZ",
                    "description": "Timezone",
                    "default": "America/New_York"
                }
            ],
            "volumes": [
                {
                    "container": "/config",
                    "description": "Image exposes volume mounted at /config",
                    "key": "config"
                },
                {
                    "container": "/data",
                    "description": "Image exposes volume mounted at /data"
                }
            ],
            "ports": []
        }
    ]
}