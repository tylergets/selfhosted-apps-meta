{
    "id": "webgrabplus",
    "name": "webgrabplus",
    "description": "[{{ project_name|capitalize }}]({{ project_url }}) is a multi-site incremental xmltv epg grabber. It collects tv-program guide data from selected tvguide sites for your favourite channels.",
    "icon": "http://www.webgrabplus.com/sites/default/themes/WgTheme/images/slideshows/EPG_fading.jpg",
    "links": [
        {
            "type": "docker",
            "url": "https://hub.docker.com/r/linuxserver/webgrabplus"
        },
        {
            "type": "docs",
            "url": "https://docs.linuxserver.io/images/docker-webgrabplus"
        }
    ],
    "containers": [
        {
            "name": "webgrabplus",
            "image": "linuxserver/webgrabplus",
            "env": [
                {
                    "id": "PUID",
                    "key": "PUID",
                    "description": "User ID",
                    "default": "100"
                },
                {
                    "id": "PGID",
                    "key": "PGID",
                    "description": "Group ID",
                    "default": "100"
                },
                {
                    "id": "TZ",
                    "key": "TZ",
                    "description": "Timezone",
                    "default": "America/New_York"
                }
            ],
            "volumes": [
                {
                    "container": "/config",
                    "description": "Where webgrabplus should store it's config files.",
                    "key": "config"
                },
                {
                    "container": "/data",
                    "description": "Where webgrabplus should store it's data files."
                }
            ]
        }
    ]
}