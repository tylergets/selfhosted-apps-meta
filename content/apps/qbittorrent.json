{
    "id": "qbittorrent",
    "name": "qbittorrent",
    "description": "The [{{ project_name|capitalize }}]({{ project_url }}) project aims to provide an open-source software alternative to ÂµTorrent. qBittorrent is based on the Qt toolkit and libtorrent-rasterbar library.",
    "icon": "https://github.com/linuxserver/docker-templates/raw/master/linuxserver.io/img/qbittorrent-icon.png",
    "links": [
        {
            "type": "docker",
            "url": "https://hub.docker.com/r/linuxserver/qbittorrent"
        },
        {
            "type": "docs",
            "url": "https://docs.linuxserver.io/images/docker-qbittorrent"
        }
    ],
    "containers": [
        {
            "name": "qbittorrent",
            "image": "linuxserver/qbittorrent",
            "env": [
                {
                    "id": "PUID",
                    "key": "PUID",
                    "description": "User ID",
                    "default": "100"
                },
                {
                    "id": "PGID",
                    "key": "PGID",
                    "description": "Group ID",
                    "default": "100"
                },
                {
                    "id": "TZ",
                    "key": "TZ",
                    "description": "Timezone",
                    "default": "America/New_York"
                }
            ],
            "volumes": [
                {
                    "container": "/config",
                    "description": "Contains all relevant configuration files.",
                    "key": "config"
                },
                {
                    "container": "/downloads",
                    "description": "Location of downloads on disk."
                }
            ],
            "ports": [
                {
                    "container": "8080",
                    "description": "WebUI"
                },
                {
                    "container": "6881",
                    "description": "tcp connection port"
                },
                {
                    "container": "6881/udp",
                    "description": "udp connection port"
                }
            ]
        }
    ]
}