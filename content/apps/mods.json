{
    "id": "mods",
    "name": "mods",
    "icon": "https://raw.githubusercontent.com/walkxcode/dashboard-icons/main/png/mods.png",
    "links": [
        {
            "type": "docker",
            "url": "https://hub.docker.com/r/linuxserver/mods"
        },
        {
            "type": "docs",
            "url": "https://docs.linuxserver.io/images/docker-mods"
        }
    ],
    "containers": [
        {
            "name": "mods",
            "image": "linuxserver/mods",
            "env": [
                {
                    "id": "PUID",
                    "key": "PUID",
                    "description": "User ID",
                    "default": "100"
                },
                {
                    "id": "PGID",
                    "key": "PGID",
                    "description": "Group ID",
                    "default": "100"
                }
            ]
        }
    ],
    "meta": {
        "readme-vars": {
            "404": "Not Found"
        }
    }
}