{
    "id": "code-server",
    "name": "code-server",
    "icon": "https://raw.githubusercontent.com/walkxcode/dashboard-icons/main/png/codeserver.png",
    "links": [
        {
            "type": "docker",
            "url": "https://hub.docker.com/r/linuxserver/code-server"
        },
        {
            "type": "docs",
            "url": "https://docs.linuxserver.io/images/docker-code-server"
        }
    ],
    "containers": [
        {
            "name": "code-server",
            "image": "linuxserver/code-server",
            "env": [
                {
                    "id": "PUID",
                    "key": "PUID",
                    "description": "User ID",
                    "default": "100"
                },
                {
                    "id": "PGID",
                    "key": "PGID",
                    "description": "Group ID",
                    "default": "100"
                },
                {
                    "id": "TZ",
                    "key": "TZ",
                    "description": "Timezone",
                    "default": "America/New_York"
                }
            ],
            "volumes": [],
            "ports": [
                {
                    "container": 8443,
                    "description": "Image exposes port 8443 over tcp",
                    "protocol": "tcp",
                    "web": true
                }
            ]
        }
    ]
}