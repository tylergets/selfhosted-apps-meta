{
    "id": "bookstack",
    "name": "bookstack",
    "icon": "https://raw.githubusercontent.com/walkxcode/dashboard-icons/main/png/bookstack.png",
    "links": [
        {
            "type": "docker",
            "url": "https://hub.docker.com/r/linuxserver/bookstack"
        },
        {
            "type": "docs",
            "url": "https://docs.linuxserver.io/images/docker-bookstack"
        }
    ],
    "containers": [
        {
            "name": "bookstack",
            "image": "linuxserver/bookstack",
            "env": [
                {
                    "id": "PUID",
                    "key": "PUID",
                    "description": "User ID",
                    "default": "100"
                },
                {
                    "id": "PGID",
                    "key": "PGID",
                    "description": "Group ID",
                    "default": "100"
                },
                {
                    "id": "TZ",
                    "key": "TZ",
                    "description": "Timezone",
                    "default": "America/New_York"
                }
            ],
            "volumes": [
                {
                    "container": "/config",
                    "description": "Image exposes volume mounted at /config",
                    "key": "config"
                }
            ],
            "ports": [
                {
                    "container": 443,
                    "description": "Image exposes port 443 over tcp",
                    "protocol": "tcp",
                    "web": true
                },
                {
                    "container": 80,
                    "description": "Image exposes port 80 over tcp",
                    "protocol": "tcp",
                    "web": true
                }
            ]
        }
    ]
}