{
    "id": "transmission",
    "name": "transmission",
    "icon": "https://raw.githubusercontent.com/walkxcode/dashboard-icons/main/png/transmission.png",
    "links": [
        {
            "type": "docker",
            "url": "https://hub.docker.com/r/linuxserver/transmission"
        },
        {
            "type": "docs",
            "url": "https://docs.linuxserver.io/images/docker-transmission"
        }
    ],
    "containers": [
        {
            "name": "transmission",
            "image": "linuxserver/transmission",
            "env": [
                {
                    "id": "PUID",
                    "key": "PUID",
                    "description": "User ID",
                    "default": "100"
                },
                {
                    "id": "PGID",
                    "key": "PGID",
                    "description": "Group ID",
                    "default": "100"
                },
                {
                    "id": "TZ",
                    "key": "TZ",
                    "description": "Timezone",
                    "default": "America/New_York"
                }
            ],
            "volumes": [
                {
                    "container": "/config",
                    "description": "Image exposes volume mounted at /config",
                    "key": "config"
                }
            ],
            "ports": [
                {
                    "container": 51413,
                    "description": "Image exposes port 51413 over tcp",
                    "protocol": "tcp",
                    "web": true
                },
                {
                    "container": 51413,
                    "description": "Image exposes port 51413 over udp",
                    "protocol": "udp",
                    "web": false
                },
                {
                    "container": 9091,
                    "description": "Image exposes port 9091 over tcp",
                    "protocol": "tcp",
                    "web": true
                }
            ]
        }
    ]
}